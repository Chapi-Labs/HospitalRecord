{% extends '::base.html.twig' %}
{% block body -%}
<div class="row">
    <div class="col-md-1"></div>
    <div class="col-md-10">
        <h1>Clasificaci√≥nAO listado completo</h1>
        <table class="records_list">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Identificador AO</th>
                    <th>Actions</th>
                </tr>
            </thead>
            <tbody>
            {{dump(entities)}}
                {% for entity in entities %}
                <tr>
                    <td><a href="{{ path('clasificacionao_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                    <td>{{ entity.identificadorAO }}</td>
                    <td>
                        <ul>
                            <li>
                                <a class="btn btn-xs btn-warning" href="{{ path('clasificacionao_show', { 'id': entity.id }) }}">show</a>
                            </li>
                            <li>
                                <a class="btn btn-xs btn-danger"href="{{ path('clasificacionao_edit', { 'id': entity.id }) }}">edit</a>
                            </li>
                        </ul>
                    </td>
                </tr>
                {% endfor %}
            </tbody>
        </table>
    </div>
    <div class="col-md-1"></div>
</div>
{% endblock %}
