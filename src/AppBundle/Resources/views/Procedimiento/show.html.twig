{% extends '::base.html.twig' %}

{% block body -%}

<div class="row">
    <div class="col-md-1"></div>
    <div class="col-md-10">
        <h1>Procedimiento</h1>
        <table class="record_properties">
            <tbody>
                <tr>
                    <th>Id</th>
                    <td>{{ entity.id }}</td>
                </tr>
                <tr>
                    <th>Descripcionprocedimiento</th>
                    <td>{{ entity.descripcionProcedimiento }}</td>
                </tr>
            </tbody>
        </table>
        <ul class="record_actions">
            <li>
                <a class="btn btn-default" href="{{ path('procedimiento') }}">
                Regresar al listado
                </a>
            </li>
            <li>
                <a class="btn btn-warning" href="{{ path('procedimiento_edit', { 'id': entity.id }) }}">
                Edit
                </a>
            </li>
            <li>{{ form(delete_form) }}</li>
        </ul>
    </div>
    <div class="col-md-1"></div>
</div>

{% endblock %}
