<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="consulta_homepage" path="/hello/{name}">
        <default key="_controller">ConsultaBundle:Default:index</default>
    </route>

    <route id="consulta_realizar" path="/consulta">
        <default key="_controller">ConsultaBundle:Consulta:consultarPaciente</default>
    </route>
</routes>
